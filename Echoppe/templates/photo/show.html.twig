{% extends 'base.html.twig' %}

{% block title %}Photo
{% endblock %}

{% block body %}
	<h1>Photo</h1>

	<table class="table">
		<tbody>
			<tr>
				<th>Id</th>
				<td>{{ photo.id }}</td>
			</tr>
			<tr>
				<th>UpdatedAt</th>
				<td>{{ photo.updatedAt ? photo.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
			</tr>
			<tr>
				<th>ImageName</th>
				<td>{{ photo.imageName }}</td>
			</tr>
			<tr>
				<th>Image</th>
				<td><img style="max-width: 500px;" src="{{ vich_uploader_asset(photo, 'imageFile') }}"></td>
			</tr>
		</tbody>
	</table>

	<a href="{{ path('app_photo_index') }}">back to list</a>

	<a href="{{ path('app_photo_edit', {'id': photo.id}) }}">edit</a>

	{{ include('photo/_delete_form.html.twig') }}
{% endblock %}

